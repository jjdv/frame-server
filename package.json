{
  "name": "frame-server",
  "version": "1.0.0",
  "description": "Modular Node.js/Express server. Use it as a quick, lightweight server or build it up easily with the features you need.",
  "main": "bin/frame-server.cli.js",
  "bin": {
    "fserver": "bin/frame-server.cli.js"
  },
  "scripts": {
    "lint": "eslint **/*.js --fix",
    "test": "mocha 'test/**/*.test.js'",
    "testc": "nyc mocha 'test/**/*.test.js'",
    "docs": "jsdoc2md %INIT_CWD%/**/*.js --plugin dmd-readable > %INIT_CWD%/doc.md",
    "postinstall": "node modules/install/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjdv/frame-server.git"
  },
  "keywords": [
    "server",
    "node",
    "express",
    "http",
    "frame",
    "modular"
  ],
  "author": "Jacek Jakubik (https://github.com/jjdv)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jjdv/frame-server/issues"
  },
  "homepage": "https://github.com/jjdv/frame-server#readme",
  "dependencies": {
    "express": "^4.17.1",
    "node-basic-helpers": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "dirty-chai": "^2.0.1",
    "dmd-readable": "^0.4.1",
    "eslint": "^6.5.0",
    "eslint-plugin-prettierx": "^0.4.0",
    "jsdoc-to-markdown": "^5.0.1",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "prettierx": "^0.7.1",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0"
  }
}
